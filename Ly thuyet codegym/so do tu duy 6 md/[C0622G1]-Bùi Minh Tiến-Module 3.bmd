                                    <node id="166" text="CREATE TABLE Customers(&#xD;&#xA;	CustomerID INT,&#xD;&#xA;	CustomerName VARCHAR(255) UNIQUE,&#xD;&#xA;	PhoneNumber INT,&#xD;&#xA;	City VARCHAR(255) UNIQUE&#xD;&#xA;);" x="3560296" y="1921001" width="389" height="124" text_b="2, 2, 385, 120" fold_btn_b="3560686, 1921057, 13, 13">
                                      <style>
