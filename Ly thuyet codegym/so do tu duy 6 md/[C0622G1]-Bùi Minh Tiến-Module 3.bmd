                                        <font font_family="Times New Roman" size="12.75" style="Regular" />
                                      </style>
                                      <nodes>
                                        <node id="16" text="CREATE TABLE Customers(&#xD;&#xA;	CustomerID INT,&#xD;&#xA;	CustomerName VARCHAR(255) UNIQUE,&#xD;&#xA;	PhoneNumber INT,&#xD;&#xA;	City VARCHAR(255) UNIQUE&#xD;&#xA;);" x="3560947" y="1921383" width="389" height="124" text_b="2, 2, 385, 120" fold_btn_b="3561337, 1921439, 13, 13">
                                          <style>
                                            <text_align>Left</text_align>
                                            <font font_family="Times New Roman" size="12.75" style="Regular" />
                                          </style>
                                          <nodes>
                                            <node id="11" text="- Hai đoạn mã nằm ở phía trên dùng để tạo cái bảng Customers, chúng đều có ràng buộc UNIQUE liên quan đến hai cột CustomerName &amp; City. &#xD;&#xA;Tuy nhiên, đối với đoạn mã thứ nhất thì bên trong cái bảng Customers có thể có nhiều khách hàng trùng tên,&#xD;&#xA; có thể có nhiều khách hàng trùng thành phố, chỉ riêng trường hợp không được phép tồn tại các khách trùng tên lẫn thành phố." x="3561416" y="1921413" width="1020" height="65" text_b="2, 2, 1016, 61">
