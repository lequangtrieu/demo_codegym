                                        </node>
                                        <node id="17" text="CREATE TABLE Customers(&#xD;&#xA;	CustomerID INT,&#xD;&#xA;	CustomerName VARCHAR(255),&#xD;&#xA;	PhoneNumber INT,&#xD;&#xA;	City VARCHAR(255),&#xD;&#xA;	CONSTRAINT Hello500 UNIQUE(CustomerName,City)&#xD;&#xA;);" x="3560947" y="1921517" width="486" height="143" text_b="2, 2, 482, 139" fold_btn_b="3561434, 1921582, 13, 13">
